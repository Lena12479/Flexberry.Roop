    }}
    {{flexberry-validationmessage error=(v-get validationModel "поставщик" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.номерПозиции
      class=(if (v-get validationModel "номерПозиции" "isInvalid") "error")
