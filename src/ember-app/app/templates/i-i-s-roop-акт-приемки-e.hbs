  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.колво
      class=(if (v-get validationModel "колво" "isInvalid") "error")
      label=(t "forms.i-i-s-roop-акт-приемки-e.колво-caption")
      data-test-i-i-s-roop-акт-приемки-e-колво=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "колво" "message")}}
  </div>
</form>
